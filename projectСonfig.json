{
    "path": {
        "src": {
            "srcPath": "./src",
            "components": "/_components",
            "html": [
                "/**/*.html"
            ],
            "style": [
                "/assets/css/app.scss"
            ],
            "script": [
                "/assets/js/app.js"
            ],
            "image": [
                "/assets/img/**/*.*"
            ],
            "svg": [
                "/assets/svg/**/*.*"
            ],
            "font": [
                "/assets/fonts/**/*.*"
            ],
            "cMap": [
                "/map/**/*.*"
            ],
            "cVue": [
                "/vue/**/*.*"
            ]
        },
        "dist": {
            "distPath": "./public",
            "html": "/",
            "style": "/assets/css/",
            "script": "/assets/js/",
            "image": "/assets/img/",
            "font": "/assets/fonts/",
            "svg": "/assets/svg",
            "cMap": "/map",
            "cVue": "/vue"
            
        }
    }
}